  <Result>
    <Input>
      <Description><![CDATA[Select(Select) -> Count(Sum)]]></Description>
      <Expression><![CDATA[query.Select(doc => doc.Multiples).Count(array => (array.Sum() > 5)))]]></Expression>
    </Input>
    <Output>
      <SqlQuery><![CDATA[
SELECT VALUE COUNT(1) 
FROM (
    SELECT VALUE root["Multiples"] 
    FROM root
) AS r0 
JOIN (
    SELECT VALUE SUM(v0) 
    FROM r0 
    JOIN v0 IN r0
) AS v1 
WHERE (v1 > 5) 
]]></SqlQuery>
    </Output>
  </Result>
